{
  "manifest_version": 3,
  "name": "Images Downloader For Yupoo",
  "version": "1.5.1",
  "description": "Download Yupoo images with original quality.",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "declarativeNetRequest",
    "tabs"
  ],
  "host_permissions": [
    "https://*.yupoo.com/*",
    "https://photo.yupoo.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  },
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}